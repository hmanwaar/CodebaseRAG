{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "PostgreSQL": "Host=your-neon-host;Database=your-db;Username=your-user;Password=your-password"
  },
  "AllowedHosts": "*",
  "Ollama": {
    "BaseUrl": "http://localhost:11434",
    "EmbeddingModel": "nomic-embed-text",
    "ChatModel": "codellama:7b",
    "RequestTimeoutMinutes": 5,
    "MaxRetries": 3,
    "RetryDelaySeconds": 2,
    "FallbackEmbeddingDimension": 384
  }
}